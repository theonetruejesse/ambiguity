{
  "name": "manipulator",
  "version": "1.0.0",
  "main": "./dist/server.js",
  "exports": {
    "./clients/next": "./dist/clients/next.js",
    "./clients/vanilla": "./dist/clients/vanilla.js"
  },
  "scripts": {
    "prebuild": "node gen-exports.js",
    "build": "webpack --config webpack.config.js --mode production",
    "start": "node dist/bundle.js",
    "watch:build": "webpack --watch --config webpack.config.js",
    "watch:start": "nodemon -w dist dist/bundle.js",
    "dev": "concurrently \"pnpm watch:build\" \"pnpm watch:start\"",
    "db:generate": "prisma migrate dev",
    "db:create": "prisma migrate dev --create-only",
    "db:migrate": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "@tanstack/react-query": "^5.62.11",
    "next": "^15.1.3",
    "@trpc/client": "^11.0.0-rc.446",
    "@trpc/react-query": "^11.0.0-rc.446",
    "@trpc/server": "^11.0.0-rc.446",
    "client-only": "^0.0.1",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.1",
    "kysely": "^0.27.5",
    "luxon": "^3.5.0",
    "pg": "^8.13.1",
    "prisma-kysely": "^1.8.0",
    "server-only": "^0.0.1",
    "source-map-support": "^0.5.21",
    "superjson": "^2.2.2",
    "react": "^18.3.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@swc/core": "^1.10.7",
    "@swc/helpers": "^0.5.15",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.6",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.18",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "prisma": "^6.1.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}