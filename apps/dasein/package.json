    "discordx": "^11.12.2",
    "luxon": "^3.5.0",
    "manipulator": "workspace:*",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "ts-loader": "^9.5.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
